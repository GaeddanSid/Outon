{
  "compilerOptions": {
    "module": "esnext",
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "dist/App.js",
    "dist/TryOnView.js",
    "dist/ProductView.js",
    "dist/LandingPage.js",
    "dist/Navbar.js",
    "src/index.js"
  ],
  "exclude": ["node_modules", "build", "dist"]
}
